<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:adaptivecards="http://schemas.android.com/apk/res-auto">
    <item adaptivecards:state_error="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="20dip" />
            <stroke
                android:width="2.5dip"
                android:color="@android:color/white" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="4dip" />
            <stroke
                android:width="2.5dip"
                android:color="@android:color/white" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>